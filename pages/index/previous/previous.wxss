.container{
  width: 100%;
  height: 100%;
}
.header{
  display: flex;
  height: 15px;
  padding: 10px 0;
  justify-content: space-around;
  font-size: 14px;
}
.list{
  display: flex;
  justify-content: space-between;
  padding: 10px;
  margin: 0 20px;
  border-bottom: 1px solid #ebebeb
}
.left{
  font-size: 14px;
  line-height: 20px;
  color: #888
}
.right {
  width: 20px;
  height: 20px;
}
image{
  width: 20px;
  height: 20px;
}
.selected{
  color: #89bdf7
}